{
  "name": "homebudgeting-backend",
  "version": "1.0.0",
  "description": "Simple home budgeting app to plan monthly budgets and track incomes, savings, monthly expenses, and daily expenses. Built with the MERN stack.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "node --watch -r dotenv/config src/index.js",
    "start": "node src/index.js",
    "lint": "eslint src --ext .js",
    "format": "prettier --write \"src/**/*.{js,json,md}\"",
    "test": "vitest",
    "test:run": "vitest run",
    "test:unit": "vitest --config vitest.config.unit.js",
    "test:unit:run": "vitest run --config vitest.config.unit.js",
    "test:docker": "vitest --config vitest.config.docker.js",
    "test:docker:run": "vitest run --config vitest.config.docker.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MuhammadHussnainShaker/homebudgeting-backend.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MuhammadHussnainShaker/homebudgeting-backend/issues"
  },
  "homepage": "https://github.com/MuhammadHussnainShaker/homebudgeting-backend#readme",
  "dependencies": {
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "firebase-admin": "^13.6.1",
    "jsonwebtoken": "^9.0.3",
    "mongoose": "^9.0.2"
  },
  "devDependencies": {
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-node": "^11.1.0",
    "mongodb-memory-server": "^11.0.1",
    "prettier": "^3.7.4",
    "supertest": "^7.2.2",
    "vitest": "^4.0.18"
  }
}
